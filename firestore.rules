rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Tests collection: allow authenticated users to read/write
    match /tests/{docId} {
      allow read, write: if request.auth != null;
    }

    // Patients collection: allow authenticated users to read/write
    match /patients/{docId} {
      allow read, write: if request.auth != null;
    }
  }
}
